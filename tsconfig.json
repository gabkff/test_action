{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "declaration": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "jsx": "preserve",
    "lib": [
      "es2022",
      "dom",
      "dom.iterable",
      "scripthost"
    ],
    "module": "es2022",
    "moduleResolution": "node",
    "noEmit": true,
    "noImplicitAny": true,
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "src/*": [
        "./src/*"
      ],
      "assets/*": [
        "./src/assets/*"
      ],
      "config": [
        "./src/config"
      ],
      "config/*": [
        "./src/config/*"
      ],
      "pages/*": [
        "./src/pages/*"
      ],
      "components/*": [
        "./src/components/*"
      ],
      "ui/*": [
        "./src/components/ui/*"
      ],
      "UiKit/*": [
        "./src/components/UiKit/*"
      ],
      "plugins/*": [
        "./src/plugins/*"
      ],
      "store/*": [
        "./src/plugins/store/*"
      ],
      "router": [
        "./src/router/index"
      ],
      "router/*": [
        "./src/router/*"
      ],
      "utils/*": [
        "./src/utils/*"
      ],
      "types/*": [
        "./src/types/*"
      ],
      "i18n": [
        "./src/plugins/i18n/index"
      ],
      "i18n/*": [
        "./src/plugins/i18n/*"
      ],
      "vendors/*": [
        "./src/vendors/*"
      ],
      "plugins/utils": [
        "./src/plugins/utils"
      ],
      "plugins/utils/*": [
        "./src/plugins/utils/*"
      ],
      "*": [
        "./src/types/*.ts",
        "./src/types/*.d.ts"
      ]
    },
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "strictBindCallApply": true,
    "strictFunctionTypes": true,
    "strictNullChecks": true,
    "target": "es2022",
    "typeRoots": [
      "./node_modules/@types",
      "./src/types"
    ],
    "types": [
      "node",
      "vite/client",
      "lodash",
      "google.maps"
    ],
    "useDefineForClassFields": true
  },
  "include": [
    "./src/**/*.ts",
    "./src/**/*.d.ts",
    "./src/**/*.tsx",
    "./src/**/*.vue",
    "./src/types/*.d.ts",
    "./src/types/*.ts"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ],
  "references": [
    {
      "path": "./tsconfig.node.json"
    }
  ]
}